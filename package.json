{
  "name": "readable",
  "publisher": "pcsoftware",
  "displayName": "Readable - AI Generated Comments",
  "icon": "logo.png",
  "description": "ðŸš€ Instantly generate detailed comments for your code using AI. Supports Javascript, TypeScript, Python, JSX/TSX, C, C#, C++, Java, and PHP",
  "author": "Puri Chapman Software <support@readable.so> (https://readable.so)",
  "license": "Apache-2.0",
  "galleryBanner": {
    "color": "#041621",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReadableLabs/readable-vscode"
  },
  "badges": [],
  "homepage": "https://readable.so",
  "version": "2.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "bugs": {
    "url": "https://github.com/ReadableLabs/readable/issues",
    "email": "support@readable.so"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "comment",
    "comments",
    "docstring",
    "autodocstring",
    "documentation",
    "completion",
    "completions",
    "javascript",
    "typescript",
    "python",
    "csharp",
    "c#",
    "c++",
    "php",
    "ai",
    "snippets",
    "snippet"
  ],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true,
      "description": ""
    }
  },
  "activationEvents": [
    "onStartupFinished",
    "onView:readable-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "readable-sidebar-view",
          "title": "Readable",
          "icon": "media/new_sidebar_icon.svg"
        }
      ]
    },
    "views": {
      "readable-sidebar-view": [
        {
          "id": "account",
          "name": "Account"
        },
        {
          "type": "tree",
          "id": "resync",
          "name": "Resync"
        },
        {
          "type": "tree",
          "id": "help",
          "name": "Help"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "readable.regenerateComment",
          "group": "inline",
          "when": "view == resync && viewItem == comment"
        }
      ],
      "view/title": [
        {
          "command": "readable.stopResync",
          "group": "navigation",
          "when": "view == resync"
        },
        {
          "command": "readable.refreshResync",
          "group": "navigation",
          "when": "view == resync"
        }
      ],
      "editor/context": [
        {
          "command": "readable.insertDocstringComment",
          "group": "1_modification"
        }
      ]
    },
    "keybindings": [
      {
        "command": "readable.insertDocstringComment",
        "key": "ctrl+'",
        "mac": "cmd+'",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "readable.stopResync",
        "title": "Readable: Stop Resync",
        "icon": "$(close)"
      },
      {
        "command": "readable.refreshResync",
        "title": "Readable: Refresh Resync",
        "icon": "$(refresh)"
      },
      {
        "command": "readable.regenerateComment",
        "title": "Readable: Regenerate Comment",
        "icon": "$(sync)"
      },
      {
        "command": "readable.enableAutoComplete",
        "title": "Readable: Enable Comment Suggestions"
      },
      {
        "command": "readable.disableAutoComplete",
        "title": "Readable: Disable Comment Suggestions"
      },
      {
        "command": "readable.login",
        "title": "Readable: Login"
      },
      {
        "command": "readable.logout",
        "title": "Readable: Logout"
      },
      {
        "command": "readable.register",
        "title": "Readable: Register"
      },
      {
        "command": "readable.resetPassword",
        "title": "Readable: Reset Password"
      },
      {
        "command": "readable.insertDocstringComment",
        "title": "Readable: Generate Docstring"
      },
      {
        "command": "readable.reportBug",
        "title": "Readable: Report a Bug"
      },
      {
        "command": "readable.giveFeedback",
        "title": "Readable: Give Feedback"
      },
      {
        "command": "readable.version",
        "title": "Readable: Version"
      },
      {
        "command": "readable.updateCount",
        "title": "Readable: Update Count",
        "icon": {
          "light": "media/light/count.svg",
          "dark": "media/dark/count.svg"
        }
      }
    ],
    "viewsWelcome": [
      {
        "view": "account",
        "contents": "Never write comments again. Use Readable to instantly generate and update comments in your code. [Learn More](https://readable.so) \n [ Log In ](command:readable.login) \n [ Sign Up ](command:readable.register)"
      },
      {
        "view": "resync",
        "contents": "Resync detects and shows out of sync comments in your source code. Any out of sync comments will show up here. \n [Learn More](https://github.com/ReadableLabs/resync) or [Open an Issue](https://github.com/ReadableLabs/readable-vscode/issues) if your language isn't supported."
      }
    ],
    "configuration": {
      "title": "Readable",
      "properties": {
        "readable.loggedIn": {
          "type": "boolean",
          "default": false,
          "description": "Enables or disables welcome view"
        },
        "readable.enableAutoComplete": {
          "type": "boolean",
          "default": true,
          "description": "Enables or disables suggestion of comments to write"
        },
        "readable.showFeedback": {
          "type": "boolean",
          "default": true,
          "description": "Enables or disables the feedback prompt after every completion."
        },
        "readable.help": {
          "type": "string",
          "enum": [
            "Join the community",
            "More info",
            "Follow us on Twitter"
          ],
          "enumDescriptions": [
            "Discord",
            "More info",
            "Twitter"
          ],
          "description": "Help"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/diff": "^5.0.2",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/sqlite3": "^3.1.8",
    "@types/vscode": "1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.2.0",
    "@opentelemetry/auto-instrumentations-node": "^0.33.1",
    "@opentelemetry/sdk-node": "^0.33.0",
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "axios": "^0.21.4",
    "axios-retry": "^3.2.4",
    "https": "^1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "react-dom-server": "^0.0.5",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  }
}
