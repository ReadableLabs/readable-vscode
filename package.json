{
  "name": "readable",
  "displayName": "Readable",
  "description": "Instantly Generate Readable Code Comments.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "url": "https://github.com/Nevin1901/codecomment_extension"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {},
    "commands": [
      {
        "command": "readable.enableAutoComplete",
        "title": "Readable: Enable Comment Suggestions"
      },
      {
        "command": "readable.disableAutoComplete",
        "title": "Readable: Disable Comment Suggestions"
      },
      {
        "command": "readable.login",
        "title": "Readable: Login"
      },
      {
        "command": "readable.register",
        "title": "Readable: Register"
      },
      {
        "command": "readable.resetPassword",
        "title": "Readable: Reset Password"
      }
    ],
    "configuration": {
      "title": "Readable",
      "properties": {
        "readable.enableAutoComplete": {
          "type": "boolean",
          "default": true,
          "description": "Enables or disables suggestion of comments to write"
        },
        "readable.showFeedback": {
          "type": "boolean",
          "default": true,
          "description": "Enables or disables the feedback prompt after every completion."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "https": "^1.0.0",
    "isomorphic-fetch": "^3.0.0"
  }
}
